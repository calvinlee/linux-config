set defsearch=google

set hintchars=hjklasdf
set tabnumbers
noremap h gT
noremap l gt
noremap j 5j
noremap k 5k
noremap d D
noremap D d
map J <C-d><CR>
map K <C-u><CR>
noremap H h
noremap L l
noremap , <C-o>
noremap . <C-i>
noremap g gg
noremap <C-,> <C-6>

set nextpattern=\s*下一页|下一张|下页\s*,\bnext\b,^>$,^(>>|››|»)$,^(>|»),(>|»)$,\bmore\b
set previouspattern=\s*上一页|上一张|上页\s*,\bprev|previous\b,^<$,^(<<|‹‹|«)$,^(<|«),(<|«)$

" PassThrough gmail and google reader
autocmd LocationChange .* js modes.passAllKeys = /.*(mail\.google\.com|www\.google\.com\/reader|www\.rememberthemilk\.com\/home|http:\/\/news.163.com\/photoview).*/.test(buffer.URL)

noremap gm :tabopen https://mail.google.com<CR>
noremap gr :tabopen https://www.google.com/reader<CR>
noremap gd :tabopen http://dict.cn/<CR>
noremap gb :tabopen http://www.blogjava.net/lihao336/<CR>
noremap ge :tabopen https://www.evernote.com/Home.action<CR>
noremap ga :tabopen http://developer.android.com<CR>

noremap gM :open https://mail.google.com<CR>
noremap gR :open https://www.google.com/reader<CR>
noremap gD :open http://dict.cn/<CR>
noremap gB :open http://www.blogjava.net/lihao336/<CR>
noremap gE :open https://www.evernote.com/Home.action<CR>
noremap gA :open http://developer.android.com<CR>

