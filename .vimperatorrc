set hintchars=hjklasdf
set tabnumbers
noremap h gT
noremap l gt
noremap j 5j
noremap k 5k
noremap ] ]]
noremap [ [[
noremap d D
noremap D d
map J <C-d><CR>
map K <C-u><CR>
noremap H h
noremap L l
noremap , <C-o>
noremap . <C-i>
noremap g gg
noremap f F
noremap F f
noremap <C-,> <C-6>

set nextpattern=\s*下一页|下一张|下页\s*,\bnext\b,^>$,^(>>|››|»)$,^(>|»),(>|»)$,\bmore\b
set previouspattern=\s*上一页|上一张|上页\s*,\bprev|previous\b,^<$,^(<<|‹‹|«)$,^(<|«),(<|«)$

" PassThrough gmail and google reader
autocmd LocationChange .* js modes.passAllKeys = /.*(mail\.google\.com|www\.google\.com\/reader|www\.rememberthemilk\.com\/home).*/.test(buffer.URL)

noremap gm :tabopen https://mail.google.com<CR>
noremap gr :tabopen https://www.google.com/reader<CR>
noremap gn :tabopen https://www.google.com/notebook<CR>
noremap gd :tabopen http://us.dict.cn/<CR>
noremap gb :tabopen http://www.blogjava.net/lihao336/<CR>

noremap gM :open https://mail.google.com<CR>
noremap gR :open https://www.google.com/reader<CR>
noremap gN :open https://www.google.com/notebook<CR>
noremap gD :open http://us.dict.cn/<CR>
noremap gB :open http://www.blogjava.net/lihao336/<CR>

